{
  "name": "yvi-soft-fullstack",
  "version": "1.0.0",
  "description": "YVI Soft website with separated frontend and backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "cd backend && node server.js",
    "dev": "node start-dev.js",
    "build": "cd backend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "deploy:backend": "node deploy-backend.js",
    "deploy:frontend": "node deploy-frontend.js",
    "deploy": "npm run build:frontend && npm run deploy:frontend",
    "deploy:full": "node deploy-full.js",
    "test:connection": "node test-frontend-backend.js",
    "test:chat": "node test-chat-full.js",
        "test:complete": "node test-complete-chat.js"
  },
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "nodemailer": "^6.9.16",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "@google/generative-ai": "^0.1.3",
    "@supabase/supabase-js": "^2.56.1"
  },
  "keywords": [
    "react",
    "nodejs",
    "fullstack",
    "website"
  ],
  "author": "YVI Soft Solutions",
  "license": "MIT"
}